<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="explore10RowModel" type="com.glosaryconstarint.app.modules.explore6.data.model.Explore10RowModel"/>
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintFrame1"
        android:layout_height="wrap_content"
        android:layout_width="@dimen/_114pxh" style="@style/groupStylegray_102cornerRadius">
        <ImageView android:id="@+id/imageIcon"
            android:layout_height="0dp"
            android:layout_marginBottom="@dimen/_6pxv"
            android:layout_marginEnd="@dimen/_8pxh"
            android:layout_marginStart="@dimen/_38pxh"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_width="@dimen/_24pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_icon_43"
            app:layout_constraintBottom_toBottomOf="@+id/txt2"
            app:layout_constraintDimensionRatio="1.0:1.0"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.51"
            app:layout_constraintStart_toEndOf="@+id/txt05Ltr" app:layout_constraintTop_toTopOf="@+id/txt05Ltr"/>
        <TextView android:ellipsize="end" android:gravity="start"
            android:id="@+id/txtA2MATEMilk"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_8pxh"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="0dp" android:singleLine="true"
            android:text="@{explore10RowModel.txtA2MATEMilk}"
            app:layout_constraintBottom_toTopOf="@+id/txt05Ltr"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageImage"
            style="@style/txtMontserratmedium12" tools:text="@string/lbl_a2mate_milk"/>
        <TextView android:ellipsize="end" android:id="@+id/txt2"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_6pxv"
            android:layout_marginEnd="@dimen/_59pxh"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{explore10RowModel.txt2}"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageIcon"
            app:layout_constraintHorizontal_bias="0.18"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txt05Ltr"
            style="@style/txtMontserratsemibold12" tools:text="@string/lbl_2"/>
        <TextView android:ellipsize="end" android:id="@+id/txt05Ltr"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{explore10RowModel.txt05Ltr}"
            app:layout_constraintBottom_toTopOf="@+id/txt2"
            app:layout_constraintEnd_toStartOf="@+id/imageIcon"
            app:layout_constraintHorizontal_bias="0.18"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtA2MATEMilk"
            style="@style/txtMontserratregular12_1" tools:text="@string/lbl_0_5_ltr"/>
        <ImageView android:id="@+id/imageImage"
            android:layout_height="0dp"
            android:layout_marginEnd="@dimen/_37pxh"
            android:layout_marginStart="@dimen/_31pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="@dimen/_46pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_image_35"
            app:layout_constraintBottom_toTopOf="@+id/txtA2MATEMilk"
            app:layout_constraintDimensionRatio="0.80701756:1.0"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
