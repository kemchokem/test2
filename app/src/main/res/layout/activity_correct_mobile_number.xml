                    app:layout_constraintEnd_toStartOf="@+id/lineLine3"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toEndOf="@+id/imageIcCaretdown"
                    app:layout_constraintTop_toTopOf="parent"
