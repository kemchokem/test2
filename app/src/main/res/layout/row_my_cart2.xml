                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="@dimen/_57pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_image"
                app:layout_constraintBottom_toTopOf="@+id/constraintFrame"
                app:layout_constraintDimensionRatio="1.0:0.6666667"
                app:layout_constraintEnd_toEndOf="@+id/imageFrame"
                app:layout_constraintHorizontal_bias="0.04"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/txtShimlaApple"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintFrame"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_67pxh"
                android:layout_width="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.27"
                app:layout_constraintStart_toEndOf="@+id/constraintGroup3400" app:layout_constraintTop_toBottomOf="@+id/imageImage">
