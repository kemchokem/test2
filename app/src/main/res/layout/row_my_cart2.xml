                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{myCart2RowModel.txt1}"
                    app:layout_constraintBottom_toBottomOf="@+id/imageIcon1"
                    app:layout_constraintEnd_toStartOf="@+id/imageIcon"
                    app:layout_constraintHorizontal_bias="0.24"
                    app:layout_constraintStart_toEndOf="@+id/imageIcon1"
                    app:layout_constraintTop_toTopOf="parent"
                    style="@style/txtMontserratsemibold24" tools:text="@string/lbl_1"/>
                <ImageView android:id="@+id/imageIcon"
                    android:layout_height="0dp"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_icon_3"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintDimensionRatio="1.0:1.0"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.6"
                    app:layout_constraintStart_toEndOf="@+id/txt1" app:layout_constraintTop_toTopOf="parent"/>
                <ImageView android:id="@+id/imageIcon1"
                    android:layout_height="0dp"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_icon_2"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintDimensionRatio="1.0:1.0"
                    app:layout_constraintEnd_toStartOf="@+id/txt1"
                    app:layout_constraintHorizontal_bias="0.31"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <ImageView android:id="@+id/imageIcon2"
                android:layout_height="0dp"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginStart="@dimen/_117pxh"
                android:layout_width="@dimen/_12pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_icon_1"
                app:layout_constraintBottom_toBottomOf="@+id/txtShimlaApple"
                app:layout_constraintDimensionRatio="1.0:1.0"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.62"
                app:layout_constraintStart_toEndOf="@+id/txtShimlaApple" app:layout_constraintTop_toTopOf="parent"/>
