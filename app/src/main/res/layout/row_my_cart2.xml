                android:layout_width="wrap_content"
                app:layout_constraintBottom_toBottomOf="@+id/imageFrame"
                app:layout_constraintEnd_toStartOf="@+id/constraintFrame"
                app:layout_constraintHorizontal_bias="0.02"
                app:layout_constraintStart_toEndOf="@+id/imageFrame" app:layout_constraintTop_toBottomOf="@+id/txt1Kg">
                <TextView android:ellipsize="end" android:id="@+id/txt2"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{myCart2RowModel.txt2}"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/txt325"
                    app:layout_constraintHorizontal_bias="0.37"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    style="@style/txtHelveticabold18" tools:text="@string/lbl_2"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txt325"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{myCart2RowModel.txt325}"
