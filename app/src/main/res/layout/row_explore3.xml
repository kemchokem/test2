<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="explore3RowModel" type="com.glosaryconstarint.app.modules.explore.data.model.Explore3RowModel"/>
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintFrame"
        android:layout_height="wrap_content"
        android:layout_width="@dimen/_114pxh" style="@style/groupStylegray_102cornerRadius">
        <ImageView android:id="@+id/imageImage"
            android:layout_height="0dp"
            android:layout_marginEnd="@dimen/_22pxh"
            android:layout_marginStart="@dimen/_21pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="@dimen/_71pxh"
            android:scaleType="fitXY"
            app:layout_constraintBottom_toTopOf="@+id/constraintFrame2"
            app:layout_constraintDimensionRatio="1.0:0.73239434"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            imageRes="@{@drawable/img_image_31}" tools:src="@drawable/img_image_31"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintFrame2"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_7pxv"
            android:layout_marginEnd="@dimen/_8pxh"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_11pxv"
            android:layout_width="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/imageImage">
            <TextView android:ellipsize="end" android:gravity="start"
                android:id="@+id/txtStrawberry"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_width="0dp" android:singleLine="true"
                android:text="@{explore3RowModel.txtStrawberry}"
                app:layout_constraintBottom_toTopOf="@+id/txt1Kg"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                style="@style/txtMontserratmedium12" tools:text="@string/lbl_strawberry"/>
            <TextView android:ellipsize="end" android:id="@+id/txt4"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_58pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{explore3RowModel.txt4}"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/imageIcon"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txt1Kg"
                style="@style/txtMontserratsemibold12" tools:text="@string/lbl_4"/>
            <TextView android:ellipsize="end" android:id="@+id/txt1Kg"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{explore3RowModel.txt1Kg}"
                app:layout_constraintBottom_toTopOf="@+id/txt4"
                app:layout_constraintEnd_toStartOf="@+id/imageIcon"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtStrawberry"
                style="@style/txtMontserratregular12_1" tools:text="@string/lbl_1_kg"/>
            <ImageView android:id="@+id/imageIcon"
                android:layout_height="0dp"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginStart="@dimen/_51pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="@dimen/_24pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_icon_42"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintDimensionRatio="1.0:0.9941667"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.56"
                app:layout_constraintStart_toEndOf="@+id/txt1Kg" app:layout_constraintTop_toTopOf="@+id/txt1Kg"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
